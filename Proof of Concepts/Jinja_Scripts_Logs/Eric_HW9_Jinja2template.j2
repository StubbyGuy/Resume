<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Server Logs</title>
</head>
<body>
    <h1>Server Logs</h1>
    <p>Page generated on: {{ now().strftime('%m/%d/%Y, %I:%M:%S %p') }}</p>

    <h1>Server 2</h1>
    {% for log in logs %}
        {% if 'server2' in log %}
            <p>{{ log }}</p>
        {% endif %}
    {% endfor %}

    <h1>Server 3</h1>
    {% for log in logs %}
        {% if 'server3' in log %}
            <p>{{ log }}</p>
        {% endif %}
    {% endfor %}

    <h1>All Other Logs</h1>
    {% for log in logs %}
        {% if 'server2' not in log and 'server3' not in log %}
            <p>{{ log }}</p>
        {% endif %}
    {% endfor %}
</body>
</html>


    

